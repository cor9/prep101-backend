{
  "version": 2,
  "builds": [
    {
      "src": "simple-backend-rag.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["methodology/**"],
        "maxDuration": 300
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "simple-backend-rag.js"
    }
  ],
  "functions": {
    "simple-backend-rag.js": {
      "maxDuration": 300
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}
